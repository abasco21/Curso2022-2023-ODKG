 @prefix bus: <http://www.miciudadamiga.madrid/resource//LineaBus/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix metro: <http://www.miciudadamiga.madrid/resource/ParadaMetro/> .
@prefix ns: <http://www.miciudadamiga.madrid/ontology/SocialCenter#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns: a owl:Ontology ;
    dc:creator "Group21" ;
    dc:description "Ontology code created by Chowlk" ;
    dc:title "Social Center Ontology" ;
    owl:versionInfo "0.0.1" .

ns:belongToDistrict a owl:ObjectProperty ;
    rdfs:label "belong to district" ;
    rdfs:domain <httpbarrio:MadridNeighborhood> .

ns:belongToNeighborhood a owl:ObjectProperty ;
    rdfs:label "belong to neighborhood" ;
    rdfs:domain ns:SocialCenter .

ns:busAccess a owl:ObjectProperty ;
    rdfs:label "bus access" ;
    rdfs:domain ns:SocialCenter .

ns:hasAccesibility a owl:DatatypeProperty ;
    rdfs:label "has accesibility" ;
    rdfs:range xsd:boolean .

ns:hasAddress a owl:DatatypeProperty ;
    rdfs:label "has address" ;
    rdfs:range xsd:string .

ns:hasDescription a owl:DatatypeProperty ;
    rdfs:label "has description" ;
    rdfs:range xsd:string .

ns:hasName a owl:DatatypeProperty ;
    rdfs:label "has name" ;
    rdfs:range xsd:string .

ns:hasSchedule a owl:DatatypeProperty ;
    rdfs:label "has schedule" ;
    rdfs:range xsd:string .

ns:hasTelophone a owl:DatatypeProperty ;
    rdfs:label "has telophone" ;
    rdfs:range xsd:int .

ns:metroAccess a owl:ObjectProperty ;
    rdfs:label "metro access" .

bus:BusLine a owl:Class ;
    rdfs:label "Bus Line" .

metro:SubwayStop a owl:Class ;
    rdfs:label "Subway Stop" .

geo:SpatialThing a owl:Class ;
    rdfs:label "Spatial Thing" .

geo:location a owl:ObjectProperty ;
    rdfs:label "location" ;
    rdfs:domain ns:SocialCenter .

<httpdistrito:District> a owl:Class ;
    rdfs:label "District" .

<httpbarrio:MadridNeighborhood> a owl:Class ;
    rdfs:label "Madrid Neighborhood" .

ns:SocialCenter a owl:Class ;
    rdfs:label "Social Center" .

