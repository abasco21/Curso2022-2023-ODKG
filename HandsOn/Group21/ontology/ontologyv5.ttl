 @prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix district: <http://www.miciudadamiga.madrid/map/resource/District/> .
@prefix metro: <http://www.miciudadamiga.madrid/map/resource/ParadaMetro/> .
@prefix neighborhood: <http://www.miciudadamiga.madrid/map/resource/Neighborhood/> .
@prefix ns: <http://www.miciudadamiga.madrid/map/ontology/SocialCenter#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns: a owl:Ontology ;
    dc:creator "Group21" ;
    dc:description "Ontology code created by Chowlk" ;
    dc:title "Social Center Ontology" ;
    owl:versionInfo "0.1.5" .

ns:accesibility a owl:DatatypeProperty ;
    rdfs:label "accesibility" ;
    rdfs:range xsd:boolean .

ns:belongsToMadridDistric a owl:ObjectProperty ;
    rdfs:label "belongs to madrid distric" ;
    rdfs:domain ns:DayCenter .

ns:belongsToMadridNeighborhood a owl:ObjectProperty ;
    rdfs:label "belongs to madrid neighborhood" ;
    rdfs:domain ns:DayCenter .

ns:busLine a owl:DatatypeProperty ;
    rdfs:label "bus line" ;
    rdfs:range xsd:string .

ns:hasMetroAccess a owl:ObjectProperty ;
    rdfs:label "has metro access" .

district:MadridDistrict a owl:Class ;
    rdfs:label "Madrid District" .

neighborhood:MadridNeighborhood a owl:Class ;
    rdfs:label "Madrid Neighborhood" .

metro:SubwayStation a owl:Class ;
    rdfs:label "Subway Station" .

<httpsch:address> a owl:DatatypeProperty ;
    rdfs:label "address" ;
    rdfs:range xsd:string .

<httpsch:description> a owl:DatatypeProperty ;
    rdfs:label "description" ;
    rdfs:range xsd:string .

<httpsch:latitude> a owl:DatatypeProperty ;
    rdfs:label "latitude" ;
    rdfs:range xsd:number .

<httpsch:longitude> a owl:DatatypeProperty ;
    rdfs:label "longitude" ;
    rdfs:range xsd:number .

<httpsch:name> a owl:DatatypeProperty ;
    rdfs:label "name" ;
    rdfs:range xsd:string .

<httpsch:openingHours> a owl:DatatypeProperty ;
    rdfs:label "opening hours" ;
    rdfs:range xsd:string .

<httpsch:telephone> a owl:DatatypeProperty ;
    rdfs:label "telephone" ;
    rdfs:range xsd:string .

<httpsch:url> a owl:DatatypeProperty ;
    rdfs:label "url" ;
    rdfs:range xsd:uRI .

<httpsch:GovernmentBuilding> a owl:Class ;
    rdfs:label "Government Building" .

ns:DayCenter a owl:Class ;
    rdfs:label "Day Center" ;
    rdfs:subClassOf <httpsch:GovernmentBuilding> .

